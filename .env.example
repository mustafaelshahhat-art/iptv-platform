# IPTV Platform Environment Variables
# Copy this file to .env and fill in your credentials

# IPTV Server Configuration (REQUIRED)
IPTV_SERVER_URL=http://your-iptv-server.com
IPTV_USERNAME=your_username
IPTV_PASSWORD=your_password

# Server Configuration (Optional)
PORT=3000

# Cache Duration in milliseconds (Optional)
# Default: 3600000 (1 hour)
CACHE_DURATION=3600000
